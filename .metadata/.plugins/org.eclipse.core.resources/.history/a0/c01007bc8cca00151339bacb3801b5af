package chess;

import javax.swing.*;

public class chess {

	public static void main(String[] args) {
		
		JFrame chess = new JFrame();
		chess.setSize(400, 400);
		chess.setVisible(true);
		JButton spot[][] = new JButton[8][8];
		
		for (int i = 0; i<8; i++){
			for (int j = 0; j<8; i++){
				spot[i][j] = new JButton();
				spot[i][j].setBounds((i+1)*50, (j+1)*50, 50, 50);
			}
		}
		
	}
	
}
